{
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "func-names": ["error", "never"],
    "no-underscore-dangle": "off",
    "dot-notation": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "camelcase": [
      "error",
      {
        "ignoreDestructuring": true,
        "properties": "never",
        "allow": ["access_token"]
      }
    ]
  }
}
